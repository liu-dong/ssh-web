<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dong.web.action.SysRolePermission" table="sys_role_permission" schema="meet">
        <id name="id">
            <column name="id" sql-type="varchar(36)" length="36"/>
        </id>
        <property name="roleId">
            <column name="role_id" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="permissionId">
            <column name="permission_id" sql-type="varchar(36)" length="36"/>
        </property>
    </class>
</hibernate-mapping>
