<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dong.web.action.OrgPerson" table="sys_org_person" schema="meet">
        <id name="id">
            <column name="id" sql-type="varchar(36)" length="36"/>
        </id>
        <property name="personId">
            <column name="person_id" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="orgId">
            <column name="org_id" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="updateTime">
            <column name="update_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
